#include <dt-bindings/gpio/gpio.h>
#include "armada-7040.dtsi"

/ {
	model = "delta,tn48m";
	compatible = "delta,tn48m";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x80000000>;
	};

	aliases {
		ethernet0 = &cp0_eth0;
		ethernet1 = &cp0_eth1;
		ethernet2 = &cp0_eth2;
	};

	sfp1: sfp-1 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp0>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 0 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 1 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 2 GPIO_ACTIVE_HIGH>;
	};

	sfp2: sfp-2 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp1>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 3 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 4 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 5 GPIO_ACTIVE_HIGH>;
	};

	sfp3: sfp-3 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp2>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 6 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 7 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 8 GPIO_ACTIVE_HIGH>;
	};

	sfp4: sfp-4 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp3>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 9 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 10 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 11 GPIO_ACTIVE_HIGH>;
	};

	sfp5: sfp-5 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp4>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 12 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 13 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 14 GPIO_ACTIVE_HIGH>;
	};

	sfp6: sfp-6 {
		compatible = "sff,sfp";
		i2c-bus = <&i2c1_sfp5>;
		maximum-power-milliwatt = <2000>;
		los-gpio = <&a7k_slim_cpld 15 GPIO_ACTIVE_HIGH>;
		mod-def0-gpio = <&a7k_slim_cpld 16 GPIO_ACTIVE_LOW>;
		tx-disable-gpio = <&a7k_slim_cpld 17 GPIO_ACTIVE_HIGH>;
	};

	prestera {
		compatible = "marvell,prestera";
		status = "okay";
		/* reg = <0x0 0x7F900000 0x0 0x40000>; */
		ports {
			port25 {
				prestera,port-num = <25>;
				sfp = <&sfp1>;
			};
			port26 {
				prestera,port-num = <26>;
				sfp = <&sfp2>;
			};

			port27 {
				prestera,port-num = <27>;
				sfp = <&sfp3>;
			};

			port28 {
				prestera,port-num = <28>;
				sfp = <&sfp4>;
			};

			port29 {
				prestera,port-num = <29>;
				sfp = <&sfp5>;
			};

			port30 {
				prestera,port-num = <30>;
				sfp = <&sfp6>;
			};
		};
	};

	onie_eeprom: onie-eeprom {
		compatible = "onie-nvmem-cells";
		status = "okay";

		nvmem = <&eeprom_at24>;
	};

	prestera {
		compatible = "marvell,prestera";
		status = "okay";

		base-mac-provider = <&onie_eeprom>;
	};
};

&uart0 {
	status = "okay";
};

&cp0_i2c0 {
	status = "okay";
	clock-frequency = <100000>;

	a7k_slim_cpld: a7k-slim-cpld@31 {
		compatible = "delta,a7k_slim_cpld";
		#address-cells = <1>;
		#size-cells = <0>;
		gpio-controller;
		#gpio-cells = <2>;
		reg = <0x31>;  /* CPLD/MUX I2C address */

		gpio-map {
			/* sfp1 */
			sfp01_gpio00_loss {
				reg-map = <0x32 (1 << 0)>;
				gpio-num = <0>;
			};
			sfp01_gpio01_pres {
				reg-map = <0x31 (1 << 0)>;
				gpio-num = <1>;
			};
			sfp01_gpio02_tx_dis {
				reg-map = <0x30 (1 << 0)>;
				gpio-num = <2>;
			};
			/* sfp2 */
			sfp02_gpio00_sfp_loss {
				reg-map = <0x32 (1 << 1)>;
				gpio-num = <3>;
			};
			sfp02_gpio01_sfp_pres {
				reg-map = <0x31 (1 << 1)>;
				gpio-num = <4>;
			};
			sfp02_gpio02_tx_dis {
				reg-map = <0x30 (1 << 1)>;
				gpio-num = <5>;
			};
			/* sfp3 */
			sfp03_gpio00_loss {
				reg-map = <0x32 (1 << 2)>;
				gpio-num = <6>;
			};
			sfp03_gpio01_pres {
				reg-map = <0x31 (1 << 2)>;
				gpio-num = <7>;
			};
			sfp03_gpio02_tx_dis {
				reg-map = <0x30 (1 << 2)>;
				gpio-num = <8>;
			};
			/* sfp4 */
			sfp04_gpio00_loss {
				reg-map = <0x32 (1 << 3)>;
				gpio-num = <9>;
			};
			sfp04_gpio01_sfp_pres {
				reg-map = <0x31 (1 << 3)>;
				gpio-num = <10>;
			};
			sfp04_gpio02_tx_dis {
				reg-map = <0x30 (1 << 3)>;
				gpio-num = <11>;
			};
			/* sfp5 */
			sfp05_gpio00_loss {
				reg-map = <0x32 (1 << 4)>;
				gpio-num = <12>;
			};
			sfp05_gpio01_pres {
				reg-map = <0x31 (1 << 4)>;
				gpio-num = <13>;
			};
			sfp05_gpio02_tx_dis {
				reg-map = <0x30 (1 << 4)>;
				gpio-num = <14>;
			};
			/* sfp6 */
			sfp06_gpio00_sfp_loss {
				reg-map = <0x32 (1 << 5)>;
				gpio-num = <15>;
			};
			sfp06_gpio01_sfp_pres {
				reg-map = <0x31 (1 << 5)>;
				gpio-num = <16>;
			};
			sfp06_gpio02_tx_dis {
				reg-map = <0x30 (1 << 5)>;
				gpio-num = <17>;
			};
		};
	};

	i2c-mux@71 {
		compatible = "nxp,pca9546";
		reg = <0x71>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
		};
		i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
		};
		i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
		};
		i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
		};
	};
};

&cp0_i2c1 {
	status = "okay";
	clock-frequency = <100000>;

	eeprom_at24: at24@51 {
		compatible = "atmel,24c64";
		reg = <0x51>;
	};

	i2cmux@73 {
		compatible = "nxp,pca9548";
		reg = <0x73>;
		#address-cells = <1>;
		#size-cells = <0>;
		i2c-mux-idle-disconnect;

		i2c1_sfp0: i2c@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;
                };
		i2c1_sfp1: i2c@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp2: i2c@2 {
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp3: i2c@3 {
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp4: i2c@4 {
			reg = <4>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp5: i2c@5 {
			reg = <5>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp6: i2c@6 {
			reg = <6>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		i2c1_sfp7: i2c@7 {
			reg = <7>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
	};
};

&cp0_spi0 {
	status = "okay";

	spi-flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0x0>;
		spi-max-frequency = <20000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "uboot";
				reg = <0x0 0x3f0000>;
			};

			partition@3f0000 {
				label = "uboot-env";
				reg = <0x3f0000 0x010000>;
			};

			partition@400000 {
				label = "ONIE";
				reg = <0x400000 0xc00000>;
			};
		};
	};
};

&cp0_sata0 {
	status = "okay";
};

&cp0_usb3_0 {
	status = "okay";
};

&cp0_usb3_1 {
	status = "okay";
};

&ap_sdhci0 {
	status = "okay";
	bus-width = <4>;
	no-1-8-v;
	non-removable;
};

&cp0_sdhci0 {
	status = "okay";
	bus-width = <4>;
	no-1-8-v;
	non-removable;
};

&cp0_mdio {
	status = "okay";

	OOB_E1512_PHY: ethernet-phy@1 {
		reg = <0x0>;
	};
};

&cp0_ethernet {
	status = "okay";
};

&cp0_eth0 {
	status = "disabled";
};

&cp0_eth1 {
	status = "disabled";
};

&cp0_eth2 {
	status = "okay";

	phy-mode = "sgmii";
	phy = <&OOB_E1512_PHY>;
	phys = <&cp0_comphy5 2>;
};

&cp0_crypto {
	status = "disabled";
};

&cp0_pcie0 {
	dma-ranges = <0x42000000 0x0 0x00000000 0x0 0x00000000 0x0 0x40000000>;
	ranges = <0x81000000 0x0 0xfb000000 0x0 0xfb000000 0x0 0xf0000
		0x82000000 0x0 0xf6000000 0x0 0xf6000000 0x0 0x1800000
		0x82000000 0x0 0xf9000000 0x0 0xf9000000 0x0 0x100000>;
	reg = <0x00 0xf2600000 0x00 0x10000 0x00 0xf7800000 0x00 0x80000>;
	phys = <&cp0_comphy0 0>;
	status = "okay";
};

